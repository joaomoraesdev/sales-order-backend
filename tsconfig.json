{
  "compilerOptions": {
    "target": "es2024",
    "module": "node16",
    "moduleResolution": "node16",
    "skipLibCheck": true,
    "strict": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "rootDir": "./",
    "sourceMap": true,
    "paths": {
      "@models/*": ["./@cds-models/*/index.ts"]
      ,"@sap/cds": ["types/cds"]
    }
  },
  "include": ["srv", "types/**/*.d.ts"],
  "exclude": ["node_modules"]
}
